    "bugs": {
        "url": "https://github.com/MostafaElGamal/Natours-Website/issues"
    },
    "homepage": "https://github.com/MostafaElGamal/Natours-Website#readme",
    "devDependencies": {
        "autoprefixer": "^9.7.4",
        "concat": "^1.0.3",
        "node-sass": "^4.13.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^7.1.0"
    },
    "dependencies": {
        "jquery": "^3.4.1",
        "live-server": "^1.2.1"
    }
}